#include <stdio.h>

int main()
{
    int n,i,f;
    printf("enter a number:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
        if((i*(i+1))==n){
            f=1;
            break;
        }
    if(f==1)
            printf("the number is a pronic number\n");
    else
            printf("number is not a pronic number\n");
}

